{
  "singleQuote": true,
  "tabWidth": 2,
  "semi": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "printWidth": 120,
  "endOfLine": "lf",
  "importOrder": [
    "^react",
    "^react-native",
    "^react-native/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@assets/(.*)$",
    "^@core/(.*)$",
    "^@components/(.*)$",
    "^@hooks/(.*)$",
    "^@locales/(.*)$",
    "^@screens/(.*)$",
    "^@utils/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true
}
